
import { createRouter, createWebHashHistory } from 'vue-router';
import Index from './views/Index.vue';

const router = createRouter({
  history: createWebHashHistory('/h5/activity/vg_activity/demo'),
  routes: [{
    path: '/',
    component: Index,
  }, {
    path: '/:catchAll(.*)',
    redirect: '/',
  }],
});

export default router;
